jobID = jobid()

### List functions ###

functionindex = {'createfolder':'filefunctions',
                'deletefolder':'filefunctions',
                'startservice':'servicefunctions',
                'stopservice':'servicefunctions'}

### Get source code of function from module ###
def functionsource(arg1):
	import inspect
	mod = __import__(functionindex[arg1])
	function = getattr(mod, arg1)
	sourcecode = inspect.getsource(function)
	return sourcecode

