jobID = jobid()

### Create FunctionIndex ###
def functionindex():
    import inspect
    import functionmatrix
    functions = inspect.getmembers(functionmatrix, inspect.isfunction)
    info = {}
    for f in functions:
        source = inspect.getsource(getattr(functionmatrix, f[0]))
        info[f[0]] = source
    return info
    ('{'createfolder':'filefunctions',
                'deletefolder':'filefunctions',
                'startservice':'servicefunctions',
                'stopservice':'servicefunctions'}

### Get source code of function from FunctionIndex ###
def functionsource(arg1):
    s = functionindex()
    sourcecode = s[arg1]
	return sourcecode

### Config Start ###

functions = ['input','function','names','here']
config = ""

for f in functions:
    config+= functionsource(f)
