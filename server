import socket

### Create TCP/IP Socket ###
tunnel = socket.socket(socket.AF_INET,socket.SOCK_STREAM)

### Connect Tunnel to Server ###
server = ((socket.gethostbyname('0.0.0.0')),50101)
tunnel.connect(server)

try:
    ### Send data in 1024 byte packages ###
    config = 'This is where the pure python script is inserted'
    tunnel.sendall(config)
finally:
    sock.close()
